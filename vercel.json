{
  "buildCommand": "flutter build web --release --dart-define=YOUTUBE_API_KEY=$YOUTUBE_API_KEY",
  "outputDirectory": "build/web",
  "framework": null,
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}
